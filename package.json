{
  "name": "imicros-modeler",
  "version": "0.0.1",
  "description": "Modeler (wrapper of bpmn.js)",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "jest --coverage --no-cache --silent",
    "test-local": "jest --coverage --no-cache"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/*.js",
      "lib/util/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dev/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dev/"
    ],
    "rootDir": ".",
    "roots": [
      "./test"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/al66/imicros-modeler.git"
  },
  "keywords": [
    "imicros",
    "Modeler"
  ],
  "author": "Andreas Leinen <andreas.leinen@imicros.de> (www.imicros.de)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/al66/imicros-modeler/issues"
  },
  "homepage": "https://github.com/al66/imicros-modeler#readme",
  "devDependencies": {
    "jest": "^27.0.6"
  },
  "dependencies": {
    "bpmn-js": "^8.8.2"
  }
}
